{
  "name": "votedotorg-state-site-tracking",
  "version": "1.0.0",
  "description": "track state website changes for vote.org",
  "main": "index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "import": "node ./bin/import-csv.js",
    "start": "node src/main.js",
    "test": "npx mocha --forbid-only",
    "test-dev": "npx mocha",
    "test-watch": "npx mocha --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BruceBGordon/votedotorg-state-site-tracking.git"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/BruceBGordon/votedotorg-state-site-tracking/issues"
  },
  "homepage": "https://github.com/BruceBGordon/votedotorg-state-site-tracking#readme",
  "dependencies": {
    "axios": "^0.20.0",
    "chai": "^4.2.0",
    "chalk": "^4.1.0",
    "cherio": "^1.0.0-rc.2",
    "csv-parse": "^4.12.0",
    "diff": "^4.0.2",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "html-to-text": "^5.1.1",
    "mocha": "^8.1.3",
    "mongoose": "^5.10.5",
    "nodemailer": "^6.4.11",
    "pdf-parse": "^1.1.1",
    "pug": "^3.0.0"
  },
  "devDependencies": {
    "husky": "^4.3.0",
    "prettier": "^2.1.2",
    "pretty-quick": "^3.0.2"
  },
  "mocha": {
    "recursive": true,
    "spec": "test/**/*.spec.js",
    "exclude": [
      "test/data/*"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "prettier": {
    "trailingComma": "all",
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true,
    "printWidth": 120
  }
}
